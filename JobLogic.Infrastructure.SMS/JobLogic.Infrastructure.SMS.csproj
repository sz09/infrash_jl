<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup Label="Globals">
    <SccProjectName>SAK</SccProjectName>
    <SccProvider>SAK</SccProvider>
    <SccAuxPath>SAK</SccAuxPath>
    <SccLocalPath>SAK</SccLocalPath>
  </PropertyGroup>

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Product>JobLogic.Infrastructure.SMS</Product>
    <Description>Retrieve SMS status and allow StatusCallbackUrl</Description>
    <PackageId>JobLogic.Infrastructure.SMS</PackageId>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
    <PackageReleaseNotes>Retrieve SMS status and allow StatusCallbackUrl</PackageReleaseNotes>
    <Version>1.170.2</Version>
    <PackageRequireLicenseAcceptance>False</PackageRequireLicenseAcceptance>
    <Company>JobLogic</Company>
    <Authors>JobLogic</Authors>
    <PackageIconUrl>https://go.joblogic.com/favicon.ico</PackageIconUrl>
    <FileVersion>1.170.2</FileVersion>
    <AssemblyVersion>1.170.2</AssemblyVersion>
    <LangVersion>8.0</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="JobLogic.Infrastructure.ServiceResponders" Version="1.1.0" />
    <PackageReference Include="libphonenumber-csharp" Version="8.12.14" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="System.Text.RegularExpressions" Version="4.3.1" />
    <PackageReference Include="Twilio" Version="5.27.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\JobLogic.Infrastructure.Contract.Extensions\JobLogic.Infrastructure.Contract.Extensions.csproj" />
    <ProjectReference Include="..\JobLogic.Infrastructure.OneOf\JobLogic.Infrastructure.OneOf.csproj" />
  </ItemGroup>
</Project>